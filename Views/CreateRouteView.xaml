<Window x:Class="Exercise.Views.CreateRouteView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CreateRouteView" Height="450" Width="800">

    <Grid>

        <StackPanel Margin="10">
            <GroupBox Header="Nhập dữ liệu từ Excel">
                <StackPanel>
                    <Button Content="Import Excel File" Command="{Binding ImportExcelCommand}" Height="30"/>
                    <DataGrid ItemsSource="{Binding ImportedRoutes}" Height="150"/>
                </StackPanel>
            </GroupBox>
        </StackPanel>

    </Grid>
</Window>